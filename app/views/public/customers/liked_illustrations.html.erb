  <div class="row">
    <div class="col-md-12">
      <h4 class="headline mt-5 ml-5">いいねした投稿</h4>
        <div class="row mt-3">
          <% @liked_illustrations.each do |post| %>
             <%= post.name %>
             <%= image_tag post.image %>
            <%#= render 'public/shared/illustration_item', post: post %>
          <% end %>
        </div>
        <div class="d-flex justify-content-center">
          <%#= paginate @liked_illustrations, theme: 'bootstrap-5' %>
        </div>
    </div>
  </div>
